
<table>
  <tr>
    <td>
      <%= form_for :post, url: post_path(@post), method: :patch do |f| %>
        <p>
          <%= f.label :title %><br>
          <%= f.text_field :title %>
        </p>

        <p>
          <%= f.label :text %><br>
          <%= f.text_area :text %>
        </p>

        <p>
          <%= f.submit %>
        </p>
      <% end %>
    </td>
  </tr>
</table>

<hr>

<%= link_to 'Back', posts_path%>